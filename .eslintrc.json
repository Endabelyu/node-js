{
  "env": {
    "node": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": ["plugin:prettier/recommended", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "func-names": ["as-needed"],
     "string": "off" 
  },
  "plugins": ["prettier"]
}
